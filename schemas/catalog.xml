<?xml version="1.0" encoding="US-ASCII"?>
<catalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="urn:oasis:names:tc:entity:xmlns:xml:catalog http://www.oasis-open.org/committees/entity/release/1.1/catalog.xsd"
         xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog"
         prefer="public">

    <!-- Our own schemas/dtds -->
    <public publicId="-//harctoolbox.org//version//en" uri="./version.dtd"/>
    <system systemId="http://www.harctoolbox.org/dtds/version.dtd" uri="./version.dtd"/>
    <public publicId="-//harctoolbox.org//properties//en" uri="./properties.dtd"/>
    <system systemId="http://www.harctoolbox.org/dtds/properties.dtd" uri="./properties.dtd"/>
    <system systemId="http://www.harctoolbox.org/schemas/girr.xsd" uri="./girr.xsd"/>
    <system systemId="http://www.harctoolbox.org/schemas/exportformats.xsd" uri="./exportformats.xsd"/>
    <system systemId="http://www.harctoolbox.org/schemas/girr_ns.xsd" uri="./girr_ns.xsd"/>

    <!-- Needed by the documentation files -->
    <public publicId="-//APACHE//DTD DocumentationV2.0//en" uri="./document-v20.dtd"/>
