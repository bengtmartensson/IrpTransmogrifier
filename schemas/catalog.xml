    <system systemId="http://forrest.apache.org/dtd/document-v20.mod" uri="./document-v20.mod"/>

    <public publicId="-//APACHE//ENTITIES Common Character Entity Sets V1.0//EN" uri="./common-charents-v10.mod"/>
    <system systemId="http://forrest.apache.org/dtd/common-charents-v10.mod" uri="./common-charents-v10.mod"/>

    <!-- The schema for this file -->
    <system systemId="http://www.oasis-open.org/committees/entity/release/1.1/catalog.xsd" uri="./catalog.xsd"/>

    <!-- Stuff referenced by other files, like girr.xsd -->
    <system systemId="http://www.w3.org/2001/xml.xsd" uri="./xml.xsd"/>
    <system systemId="http://www.w3.org/2002/08/xhtml/xhtml1-strict.xsd" uri="./xhtml1-strict.xsd"/>
    <system systemId="https://www.w3.org/2001/XInclude/XInclude.xsd" uri="./XInclude.xsd"/>

</catalog>
